{
  "name": "podcastflow-pro",
  "version": "1.0.0",
  "description": "Next-generation podcast advertising management platform",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "./scripts/optimize-build.sh && ./scripts/build-wrapper.sh && ./scripts/post-build.sh",
    "build:fast": "NEXT_TELEMETRY_DISABLED=1 NODE_OPTIONS='--max-old-space-size=4096' next build && ./scripts/post-build.sh",
    "build:local": "BUILD_TIMEOUT_SEC=900 ./scripts/optimize-build.sh && ./scripts/build-wrapper.sh && ./scripts/post-build.sh",
    "start": "NODE_ENV=production next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:auth": "./scripts/run-tests.sh auth",
    "test:campaigns": "./scripts/run-tests.sh campaigns",
    "test:billing": "./scripts/run-tests.sh billing",
    "test:analytics": "./scripts/run-tests.sh analytics",
    "test:master": "./scripts/run-tests.sh master",
    "test:coverage": "./scripts/run-tests.sh coverage",
    "test:all": "./scripts/run-tests.sh",
    "test:contracts": "./scripts/test-contract-billing.sh",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:setup": "./scripts/setup-database.sh",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "email:seed-templates": "tsx src/scripts/seed-email-templates.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
